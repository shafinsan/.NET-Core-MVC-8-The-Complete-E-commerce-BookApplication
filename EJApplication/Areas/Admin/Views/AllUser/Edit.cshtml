@{
}
@model ApplicationUserAndRole
<form method="post">
    <input asp-for="@Model.myModel.Id" name="Id" />
    <div class="form-group">
        <label asp-for="@Model.myModel.Name"  class="control-label">Name</label>
        <input asp-for="@Model.myModel.Name" name="Name" class="form-control" />
    </div>
    <div class="form-group">
        <select asp-for="@Model.myModel.tempRole" asp-items="@Model.Role" name="myRole" class="form-control">
            <option selected disabled>--Select Category--</option>
        </select>
    </div>
    <div class="form-group my-2">
        <button type="submit" class="btn btn-primary btn-sm mx-1">Save Changes</button>
        <a asp-controller="AllUser" asp-action="Index" class="btn btn-dark btn-sm mx-1">Back</a>
    </div>
</form>